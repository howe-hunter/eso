{["internalId"]="5283",["name"]="Pledge: Crypt of Hearts I",["level"]="50",["type"]="15",["zone"]="Rivenspire",["objective"]="Dungeon: Crypt of Hearts I",["repeatType"]="2",["displayType"]="2",["backgroundText"]="Glirion the Redbeard of the Undaunted has challenged me to explore the ruined halls of the Crypt of Hearts. It lies beneath the stones of Rivenspire, a former school for might and magic.",["poiIndex"]="17",["isShareable"]="0"},
{["internalId"]="6379",["name"]="A Waking Nightmare",["level"]="50",["type"]="1",["zone"]="Northern Elsweyr",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="An ancient dro-m'Athra called Zalsheem lurks in the ruins of a Hidden Moon shrine south of Predator Mesa. Her corrupting influence will surely spread if I don't act quickly.",["poiIndex"]="294967291",["isShareable"]="1"},
