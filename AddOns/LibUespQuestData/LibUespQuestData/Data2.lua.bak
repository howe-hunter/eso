{["internalId"]="2558",["name"]="The Slavers",["level"]="50",["type"]="0",["zone"]="Stormhaven",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Margot Oscent has a son named Lucas who's missing, and she's worried he may have been taken by the slavers.",["poiIndex"]="294967291",["isShareable"]="1"},
