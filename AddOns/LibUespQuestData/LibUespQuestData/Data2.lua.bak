{["internalId"]="3496",["name"]="Scavenging for a Scarab",["level"]="50",["type"]="0",["zone"]="Bangkorai",["objective"]="Old Tower",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I have run into a strange man called Mazrahil the Sly Scarab who was injured. He asked me to search Imperials for documents which are important.",["poiIndex"]="28",["isShareable"]="1"},
{["internalId"]="4986",["name"]="A Token Trophy",["level"]="50",["type"]="0",["zone"]="Bangkorai",["objective"]="Old Tower",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Captain Wardush, an injured Orcish scout near Old Tower, seeks to rally others to his banner by acquiring Captain Helenus's Helm from the Seventh Legion Captain Helenus who devastated his Orcish brethren.",["poiIndex"]="28",["isShareable"]="1"},
{["internalId"]="2344",["name"]="Snakes in the Sands",["level"]="50",["type"]="0",["zone"]="Alik'r Desert",["objective"]="Ogre's Bluff",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A caravan was attacked by snakes at the oasis near Ogre's Bluff. The caravan guard is afraid of snakes and asked me to return and help the members of the caravan.",["poiIndex"]="33",["isShareable"]="1"},
{["internalId"]="4760",["name"]="Whose Wedding?",["level"]="50",["type"]="0",["zone"]="Alik'r Desert",["objective"]="Sep's Spine",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The bandits at Sep's Spine have canceled hostilities for sake of the boss's daughter's wedding.",["poiIndex"]="8",["isShareable"]="1"},
{["internalId"]="2635",["name"]="The Elder Scroll of Alma Ruma",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="Grand Warlord Dortene reports that the Elder Scroll of Alma Ruma is being held by the enemy.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2936",["name"]="Capture Ash Mine",["level"]="33",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Ash Mine for the Dominion.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4372",["name"]="Goblin Marq",["level"]="50",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Grubby Grunyun wants me to look for his friend, Marq, who seems to think he's a Goblin.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4471",["name"]="Izad's Treasure",["level"]="50",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I dug up a chest by the shipwreck near Saintsport. It contained a shovel and directions written by a Captain Izad. It supposedly contains clues to a treasure buried on Stros M'Kai.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4382",["name"]="Moment of Truth",["level"]="50",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Dugroth was sent by his mother to prove his courage. He'd like me to help him.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4381",["name"]="Sphere Assembly",["level"]="50",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Rulorn in Port Hunding has offered a bounty for Dwarven parts.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4511",["name"]="Dead Man's Wrist",["level"]="50",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="In a cave full of plunder, I found a dead man clutching a note to his sister. Though the bracelet accompanying the note has little monetary value, his sister may be glad to have it back.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6156",["name"]="Snuffing Out the Light",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Divine Prosecution has requested I travel to Eton Nir Grotto and destroy the cocoons of the Daedra roaming the cavern. I'll also need to burn any sacrilegious text I find.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6083",["name"]="Taming the Wild",["level"]="50",["type"]="1",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Justiciar Farowel tasked me with killing Caanerin, the indrik matriarch, and restraining her spirit by activating warding stones scattered around her ancestral glade.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3224",["name"]="Capture Castle Alessia",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Castle Alessia for the Covenant.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2941",["name"]="Capture Kingscrest Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Kingscrest Mine for the Dominion.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3220",["name"]="Capture Kingscrest Keep",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Kingscrest Keep for the Covenant.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4292",["name"]="A Ritual in Smokefrost Peaks",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Worm Cult is performing a ritual at an altar somewhere in the Smokefrost Peaks.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3258",["name"]="Capture Kingscrest Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Kingscrest Mine for the Covenant.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2775",["name"]="Scout Kingscrest Keep",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Kingscrest Keep for the Covenant.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5891",["name"]="Falkreath's Demise",["level"]="50",["type"]="5",["zone"]="Craglorn",["objective"]="Dungeon: Falkreath Hold",["repeatType"]="0",["displayType"]="2",["backgroundText"]="Falkreath Hold is under siege by an army of Reachmen and minotaurs. It may well fall without outside intervention, and soon.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="4832",["name"]="Council of the Five Companions",["level"]="50",["type"]="2",["zone"]="The Harborage",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="I've been summoned back to the Harborage in preparation for our battle against Molag Bal.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4432",["name"]="Blood and Sand",["level"]="50",["type"]="5",["zone"]="Alik'r Desert",["objective"]="Dungeon: Volenfell",["repeatType"]="0",["displayType"]="2",["backgroundText"]="Raiders have attacked the camp of Tharayya, a Redguard woman I met. They killed many of her companions, and are barring her path.",["poiIndex"]="42",["isShareable"]="1"},
{["internalId"]="4555",["name"]="Blood Relations",["level"]="50",["type"]="5",["zone"]="Glenumbra",["objective"]="Dungeon: Spindleclutch II",["repeatType"]="0",["displayType"]="2",["backgroundText"]="Guardian Sud-Hareem and his cohorts have taken up residence in Spindleclutch. Guardian Sud-Hareem has returned to find his home overrun, and his son At'avar missing.",["poiIndex"]="66",["isShareable"]="1"},
{["internalId"]="2817",["name"]="Scout Faregyl Farm",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Faregyl Farm for the Covenant.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="6136",["name"]="The Vault of Moawita",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Relicmaster Glenadir of the Psijic Order has tasked me with recovering stolen relics, taken from the Vault of Moawita.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5337",["name"]="A Question of Succession",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="Shatul Range",["repeatType"]="0",["displayType"]="10",["backgroundText"]="A mortal wound to Chief Ogzor has thrown Shatul clan into crisis.",["poiIndex"]="27",["isShareable"]="1"},
{["internalId"]="5469",["name"]="Blood Price",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Rogzesh, the hearth-wife to Chief Ogzor, believes he was murdered. She suspects Glagosh, Chief Ogzor's hunt-wife, conspired to kill him. Rogzesh is convinced Glagosh secretly worships Boethiah, the Daedric Prince of Treachery.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5445",["name"]="A Treasure in Need of a Home",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found some ancient Nord relics. I'm sure someone will want to examine these. I'll take them with me and try to locate an interested party.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5497",["name"]="Atypical Artistry",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Doranar, an artist, believes the ogres of Wrothgar possess a misunderstood artistic genius. He wishes to create a replica of ogre artwork for the House of Orsimer Glories.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5458",["name"]="In the Name of the King",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I received another message from King Kurog. He requested that I come to the throne room at Scarp Keep. Apparently, he has a new mission he needs me to deal with.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5348",["name"]="To Save a Chief",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="High Priestess Solgra sent Eveli to recruit me for some sort of secret mission. It appears to involve a rescue, but she wants to give me the details in person.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5484",["name"]="Of Sentimental Value",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="One of the survivors of the frozen fleet asked me to keep an eye open for a gold pendant he lost. It was meant to be a gift for his betrothed when they are finally reunited.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5446",["name"]="Where Loyalty Lies",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="Frozen Fleet",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Lady Sovelle, an ambassador from Daggerfall, was sent to negotiate a trade agreement with the Orcs. But the fleet was blown off course and wrecked on the ice, and now Lady Sovelle is missing.",["poiIndex"]="30",["isShareable"]="1"},
{["internalId"]="5340",["name"]="Blood and the Sacred Words",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="Fharun Stronghold",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Vosh Rakh attacked Fharun Stronghold, killing people in Trinimac's name. A dying archivist claimed they sought a secret sanctuary and the sacred words of Malacath that are hidden within.",["poiIndex"]="9",["isShareable"]="1"},
{["internalId"]="5468",["name"]="The Anger of a King",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="King Kurog seems to be in a foul mood, which has affected everyone in the royal court. His shield-wife indicates that tensions at the keep are on the verge of boiling over. I wonder what happened to darken the king's usually boisterous spirits?",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5444",["name"]="Sorrow's Kiss",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="Sorrow",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Lady Laurent and Stibbons have been hired by the House of Orsimer Glories to locate the shrine of Torug gro-Igron, which supposedly sits at the top of the mountain named Sorrow.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="5349",["name"]="The King's Gambit",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="At Paragon's Remembrance, I learned that the Vosh Rakh plan to harm Chief Bazrag and High Priestess Solgra before they attack the chiefs at the king's moot in Orsinium. I need to find a way to stop them.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5481",["name"]="Blood on a King's Hands",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="All of the paths into the moot have been sealed except the one through the temple in Orsinium. Chief Bazrag wants me to get to the temple, help High Priestess Solgra, and then reach the moot before Kurog assassinates the clan chiefs.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5494",["name"]="Long Live the King",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Chief Bazrag wants to honor the memory of Kurog with a traditional Orc requiem for the dead. He believes such a ceremony will help heal the wounds Kurog inflicted on Orsinium and the clans.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5319",["name"]="Birdsong's Curse",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="According to a note I discovered in Coldperch Cavern, a hagraven named Kraala uses a totem to transform people into birds.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6007",["name"]="A Call For Aid",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a notice stating that adventurers looking for rewarding work should report to the Hall of Justice.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5803",["name"]="Divine Conundrum",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vivec City",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vivec of the Tribunal has noticed your exploits and requests an audience. Captain Jenassa's ship travels to Seyda Neen from such locations as Daggerfall, Davon's Watch, and Vulkhel Guard. When you arrive, find Canon Llevule outside the town.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="5950",["name"]="The Ancestral Tombs",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Librarian Bradyn seeks the location of the lost Library of Andule, the secret repository of knowledge of the ancient Great Houses, by building a miniature reproduction of Vvardenfell.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5832",["name"]="Like Blood from a Stone",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vassir-Didanat Mine",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A Dark Elf businessman named Malur Rethan is convinced that his brother and partner, Benar, is hiding something in the Vassir-Didanat ebony mine. He asked me to investigate.",["poiIndex"]="47",["isShareable"]="1"},
{["internalId"]="5903",["name"]="The Memory Stone",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="An elderly Dark Elf named Gilan Lerano contracted me to transport a memory stone to locations across Vvardenfell. He wishes to archive his memories for future generations.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5887",["name"]="Fleeing the Past",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Balmora",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A House Redoran Councilor seeks information regarding his missing daughter, Veya Releth. After exhausting House resources, he's turned to outside help in a concerted effort to find her.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="5919",["name"]="Of Faith and Family",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Balmora",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Even though I confirmed that Veya is safe, Councilor Eris continues to press me to arrange a meeting between him and his daughter.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="5948",["name"]="Family Reunion",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Balmora",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Veya thinks we should go and find Vatola Telem's squad that's scouting the old fort outside of Balmora. She said the fort's overrun with cultists and we need to reach the soldiers before anything happens to them.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="5857",["name"]="A Smuggler's Last Stand",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The feared Camonna Tong crime syndicate attacked a group of people smugglers at Khartag Point. Khartag and a group of slaves retreated to a nearby cavern. They'll be killed if help doesn't arrive soon.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5885",["name"]="Ancestral Ties",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Ald'ruhn",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A Dark Elf named Drelyth Hleran requested my aid in finding his family's lost grave site. He believes the Ashlanders camped near Ald'ruhn might know more about the lost tomb, but the tribes only share their secrets with those who earn their respect.",["poiIndex"]="48",["isShareable"]="1"},
{["internalId"]="5880",["name"]="Divine Inquiries",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vivec City",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Lord Vivec fears that the mysterious affliction that drains his divine energy may be the result of an enemy's attack. He asked me to work with Archcanon Tarvus to determine the source of the attack by conducting investigations throughout Vvardenfell.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="5877",["name"]="An Armiger's Duty",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Molag Mar",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A Buoyant Armiger warned me of pilgrims being attacked on the roads and directed me to speak to Captain Naros at Molag Mar if I wanted to know more.",["poiIndex"]="74",["isShareable"]="1"},
{["internalId"]="6008",["name"]="Ashlander Relations",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Zainab tribe has encouraged other Ashlanders to seek a positive relationship with outlanders. Because I've shown respect for the Ashlander culture, I've been encouraged to help build that relationship.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5872",["name"]="A Melodic Mistake",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Gnisis",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Gnisis Egg Mine has been shuttered. Some kind of mysterious illness has taken hold there. Unfortunately, many workers remain trapped inside.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="5883",["name"]="Hatching a Plan",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Gnisis",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Another crisis looms in the Gnisis Egg Mine. Kwama workers, driven mad by the tonal magicks of the Dwarven resonator, killed their queen. Without the queen, the colony dies. If I do not act, the mining town of Gnisis will die along with it.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="5864",["name"]="Nothing to Sneeze At",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Tilenra, a Telvanni healer and herbalist, has served the people of Suran for years. Unfortunately, a Mages Guild healer named Menaldinion has opened a shop of his own, and he's taken a keen interest in disrupting Tilenra's business.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5862",["name"]="The Scarlet Judge",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Suran",["repeatType"]="0",["displayType"]="0",["backgroundText"]="According to a masked vigilante known as the Scarlet Judge, a corrupt marshal named Hlaren is oppressing the citizens of Suran. I must aid the Judge in rooting out this corruption.",["poiIndex"]="14",["isShareable"]="1"},
{["internalId"]="5893",["name"]="Divine Intervention",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vivec City",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Lord Vivec's condition continues to deteriorate as his energy drains away. Archcanon Tarvus believes that Chodala's staff holds the key to saving the Warrior-Poet. However, it also makes the Ashlander practically impervious to harm.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="5902",["name"]="Divine Disaster",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vivec City",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Daedric hound Barbas tricked us into depleting Lord Vivec's energy so he could determine the location of Clockwork City. Before we deal with Barbas, we need to restore enough energy to Vivec to keep the moonlet from plummeting out of the sky.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="5905",["name"]="Divine Restoration",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vivec City",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Archcanon Tarvus revealed himself to be Barbas, the hound of Clavicus Vile. In an effort to locate and take control of Clockwork City, he tricked me into helping him drain Lord Vivec's energy. I need to stop Barbas and recover Lord Vivec's energy.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="6003",["name"]="Divine Blessings",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Vivec City",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Lord Vivec wants to reward those who helped restore his power and save Vvardenfell from Barbas and Clavicus Vile. He asked me to help him accomplish that.",["poiIndex"]="75",["isShareable"]="1"},
{["internalId"]="5882",["name"]="Ancestral Adversity",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Veloth Ancestral Tomb",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Elfbetta, personal assistant to the famed explorer Narsis Dren, informed me that the adventurer is investigating the ancestral tombs that dot the Vvardenfell landscape. They apparently uncovered an old scroll that hints at hidden treasure.",["poiIndex"]="64",["isShareable"]="1"},
{["internalId"]="5886",["name"]="The Heart's Desire",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="Nchuleftingth",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The scholar, Neramo, has a problem. One of his less savory competitors, Artisan Lenarmen, stole his Dwemer control rod, and seeks to use it to find an ancient artifact. I must stop Lenarmen before he discovers the secret of Nchulefthingth.",["poiIndex"]="34",["isShareable"]="1"},
{["internalId"]="6135",["name"]="Old Wounds",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Russafeld",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A prominent High Elf named Halimorion was murdered by a folkloric assassin called the Ghost of the Green. But there may be more to this case than meets the eye.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="3152",["name"]="Capture Faregyl Lumbermill",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Faregyl Lumbermill for the Pact.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6118",["name"]="Lauriel's Lament",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Direnni Acropolis",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met a High Elf bailiff outside the ruins of the Direnni Acropolis. He told me that an ancient curse lingers within its walls. I must find the source of this corruption and cleanse it.",["poiIndex"]="10",["isShareable"]="1"},
{["internalId"]="6064",["name"]="Casting the Bones",["level"]="50",["type"]="5",["zone"]="Bangkorai",["objective"]="Fang Lair",["repeatType"]="0",["displayType"]="2",["backgroundText"]="According to the Undaunted Trailblazer, Yisareh, the ruins of Fang Lair have been taken over by a cult of necromancers who are preparing for something big. She's enlisted my help in discovering and foiling their plans.",["poiIndex"]="64",["isShareable"]="1"},
{["internalId"]="6121",["name"]="Untamed and Unleashed",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Sil-Var-Woad",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met a keeper of the Royal Menagerie in Sil-Var-Woad. He warned me that dangerous animals have escaped into the park grounds, but he is searching for a tame indrik that may have escaped during the chaos.",["poiIndex"]="12",["isShareable"]="1"},
{["internalId"]="6177",["name"]="Gjadil's Legacy",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Three descendants of Gjadil the Keel-Hauler have traveled to Summerset to collect her buried treasure. Unable to trust each other after a death of a party member, they've requested I find the chest and bring it back to them.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4346",["name"]="Nobles' Rest",["level"]="50",["type"]="5",["zone"]="Eastmarch",["objective"]="Dungeon: Direfrost Keep",["repeatType"]="0",["displayType"]="2",["backgroundText"]="I must discover what has happened to the missing nobles of Direfrost Keep.",["poiIndex"]="41",["isShareable"]="0"},
{["internalId"]="5826",["name"]="Madness in Reaper's March",["level"]="50",["type"]="3",["zone"]="Reaper's March",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Alvur Baren of the Mages Guild wants me to recover a dangerous relic from Reaper's March.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6178",["name"]="The Forest Vandal",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A High Elf named Vinafwe is being plagued by a vandal who is ruining her carefully arranged botanical grounds with foreign flowers and even dangerous stranglers.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6180",["name"]="Obedience Issues",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I encountered a noblewoman along the beach who has lost her husband and her prized hound. I have agreed to help reunite them with her.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5841",["name"]="At Any Cost",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Mistress Dratha, having woken from a crippling illness, made it clear that she wants to beat death. Seven Daedric relics called the Stones of Cold Fire may be the key to achieving immortality.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5930",["name"]="Relics of Ashalmawia",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Numani-Rasi of the Urshilaku Ashlander tribe has requested that I explore the ruins of the Daedric shrine Ashalmawia and gather as many historic relics as I can find.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6111",["name"]="Murder In Lillandril",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Lillandril",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Sapiarchs of Lillandril imprisoned a member of the Mages Guild. The authorities are convinced this mage murdered one of Lillandril's sapiarchs, but the particulars seem murky. There may be more to this murder than meets the eye.",["poiIndex"]="9",["isShareable"]="1"},
{["internalId"]="6115",["name"]="Illusions of Grandeur",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Illumination Academy",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Students in the Illumination Academy have taken on bizarre personae and don't appear to know who or where they are. I must help them reclaim their lost identities.",["poiIndex"]="17",["isShareable"]="1"},
{["internalId"]="6142",["name"]="The Tower Sentinels",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Sea Sload altered the Ritemaster's memories and forced him to steal the Heart of Transparent Law and hide it away. While he works to unlock his memories, he wants Valsirenn and I to warn the keepers of the Crystal Tower, the Sapiarchs.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6137",["name"]="Looting the Light",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Seeks-the-Dark was hired to steal tokens from a cult within Eton Nir Grotto. Unexpectedly, the cult has been massacred by Daedra who now lurk within. I've been hired to collect the tokens and find Seek-the-Dark's assistant, Erilthel.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6109",["name"]="The Dreaming Cave",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Ritemaster used scrying magic to recall some of his missing memories. He took the Heart of Transparent Law into the Dreaming Cave beneath Ceporah Tower, but he still can't remember what happened after that. I need to help him remember.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3918",["name"]="Circus of Cheerful Slaughter",["level"]="50",["type"]="3",["zone"]="Mournhold",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="Sheogorath's book is supposed to lead me to the next trial, \"the Circus of Cheerful Slaughter.\" Let's see what the Mad God has planned for me this time.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4426",["name"]="Stacking the Odds",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Sylvian Herius asked me to deliver orders to a rebel spy in Imperial-held Cheydinhal.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4422",["name"]="Spice",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Vyctoria Girien asked me to taint Imperial food stores with a special spice.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4417",["name"]="Bloodied Waters",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Imperial archers are preventing rebel troops from crossing the river.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5534",["name"]="Cleaning House",["level"]="50",["type"]="3",["zone"]="Hew's Bane",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Zeira, the guildmaster of the Abah's Landing Thieves Guild, believes someone sold out the guild to the Iron Wheel. She needs my help to deal with the matter.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3953",["name"]="Chateau of the Ravenous Rodent",["level"]="50",["type"]="3",["zone"]="Stormhold Guild Hall",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="I've been tasked with finding another book for the Mages Guild. Once again I need to return to the Shivering Isles, to a place called the Chateau of the Ravenous Rodent.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3997",["name"]="The Mad God's Bargain",["level"]="50",["type"]="3",["zone"]="Windhelm",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="Valaste and Shalidor have finished analysis of Sheogorath's books. Now I need to help them take possession of Eyevea, the Mages Guild Sanctuary.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4971",["name"]="The Arch-Mage's Boon",["level"]="50",["type"]="3",["zone"]="Eyevea",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="After all we've been through together, Arch-Mage Shalidor wants to offer me a kind of memory boon, a way to recall every book I've ever read.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5831",["name"]="Madness in the Rift",["level"]="50",["type"]="3",["zone"]="The Rift",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Alvur Baren of the Mages Guild wants me to recover a dangerous relic from the Rift.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4434",["name"]="For Piety's Sake",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Sylvian Herius's troops have written petitions, asking the gods' intervention on the rebel side.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3850",["name"]="The Covenant Infiltrator",["level"]="50",["type"]="5",["zone"]="Bangkorai",["objective"]="Razak's Wheel",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I've encountered a Covenant scout named Damrina in Razak's Wheel. She was captured by Imperials and has asked for my aid.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="3990",["name"]="A Beginning at Bleakrock",["level"]="3",["type"]="0",["zone"]="Bleakrock Isle",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6127",["name"]="Half-Formed Understandings",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Ulliceta, an apprentice studying under the Psijics, took a powerful seeing stone called the Eye of Remembrance to the forbidden Traitor's Vault. It may reveal the secrets hidden there.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6144",["name"]="Pearls Before Traitors",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a note by a dead Dominion soldier, written by Captain Telomure. It describes a betrayal by Justiciar Avanaire. She's now hiding within the ruins of Tor-Hame-Khard.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4104",["name"]="In Search of Kireth Vanos",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Mzulft",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Raynor and Kireth Vanos, self-proclaimed Dwemer experts, left Morrowind to look for long lost knowledge and lore within Skyrim's Dwarven ruins.",["poiIndex"]="10",["isShareable"]="1"},
{["internalId"]="4105",["name"]="Kireth's Amazing Plan",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Mzulft",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Kireth Vanos has been exploring the ruins of Mzulft, looking for a valuable piece of forgotten lore. Her brother Raynor and her plan to use their findings to get invited into either Shad Astula or the Mages Guild.",["poiIndex"]="10",["isShareable"]="1"},
{["internalId"]="4722",["name"]="Welcome to Cyrodiil",["level"]="10",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've arrived in Cyrodiil, ready to help the Pact win the war.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5075",["name"]="Hilan's Invitation",["level"]="11",["type"]="3",["zone"]="Davon's Watch",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Hall Steward Hilan has asked I speak with him about the Fighters Guild in the Davon's Watch Guildhall.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5074",["name"]="Rudrasa's Invitation",["level"]="11",["type"]="3",["zone"]="Davon's Watch",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Magister Rudrasa has asked I speak with her about the Mages Guild in the Davon's Watch guildhall.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6113",["name"]="Lost in Translation",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Ritemaster Iachesis remembers a woman's voice, whispering ancient words over and over. He thinks these words are tied to the place where he hid the Heart of Transparent Law. Before we breach the barrier, we need to determine what the words mean.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6117",["name"]="The Taste of Fear",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Cey-Tarn Keep",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The garrison at Cey-Tarn Keep has been terrorized by a spate of unusual disappearances and deaths. A monster hunter named Silurie has enlisted my help to stop the killings.",["poiIndex"]="14",["isShareable"]="1"},
{["internalId"]="6146",["name"]="Wasting Away",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Corgrad Wastes",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A group of outcasts living in the seclusion of the Corgrad Wastes has seen their number dwindle after a string of disappearances. They believe the source of the trouble lies in the old buried ruins exposed by the recent sinkholes plaguing the region.",["poiIndex"]="18",["isShareable"]="1"},
{["internalId"]="6125",["name"]="A Necessary Alliance",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="With the Ritemaster dead and the Heart of Transparent Law in Nocturnal's hands, events have taken a grim turn. I need to help the Psijic sages figure out a way to enter the Crystal Tower and stop Nocturnal.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6126",["name"]="The Crystal Tower",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I negotiated an alliance of sorts with Clavicus Vile and Mephala. They agreed to help us get into the Crystal Towerâ€”mostly. Once inside, I need to find a way to stop Nocturnal before she uses the tower to remake reality.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6153",["name"]="A New Alliance",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="With the Crystal Tower saved, Valsirenn wishes to call a meeting of Summerset's most influential leaders to ensure its future protection.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4478",["name"]="Into the Hills",["level"]="11",["type"]="0",["zone"]="Betnikh",["objective"]="Carved Hills",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Laganakh senses evil spreading on Betnikh. We need to know what we're facing before we can fight it. A totem may grant me visions that will help us.",["poiIndex"]="2",["isShareable"]="1"},
{["internalId"]="4547",["name"]="Prove Your Worth",["level"]="12",["type"]="0",["zone"]="Betnikh",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Garnikh bet his axe that I can't best the Seamount hunters in duels.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4468",["name"]="Unearthing the Past",["level"]="12",["type"]="0",["zone"]="Betnikh",["objective"]="Grimfield",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Bloodthorn Cult has invaded an ancient Breton graveyard in order to raise zombies. The Seamount Orcs are concerned but await reinforcements.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="4525",["name"]="Tormented Souls",["level"]="12",["type"]="0",["zone"]="Betnikh",["objective"]="Moriseli",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Orc spirits have risen around Moriseli. I've been asked to investigate the source of this disturbance.",["poiIndex"]="6",["isShareable"]="1"},
{["internalId"]="4964",["name"]="Scion of the Blood Matron",["level"]="50",["type"]="0",["zone"]="Blood Matron's Crypt",["objective"]="",["repeatType"]="1",["displayType"]="1",["backgroundText"]="I was bitten by a vampire. I have the option of embracing the disease or receiving the cure.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3263",["name"]="Capture Faregyl Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Faregyl Mine for the Covenant.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5227",["name"]="Kill Enemy Dragonknights",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Grand Warlord Dortene has authorized a reward for any Covenant soldier who slays 20 enemy Dragonknights.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2983",["name"]="Capture Drakelowe Lumbermill",["level"]="50",["type"]="7",["zone"]="",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Drakelowe Lumbermill for the Dominion.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4277",["name"]="Silent Village",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="Shattered Grove",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I discovered a statue outside the small town of Shattered Grove. The statue is remarkably lifelike and warm to the touch.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="4278",["name"]="A Village Awakened",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="Shattered Grove",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Merormo senses that a powerful Daedra has wrested control of the animals. As long as the Daedra remains, Merormo won't free the villagers.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="4107",["name"]="Banishing the Banished",["level"]="50",["type"]="5",["zone"]="",["objective"]="Dungeon: The Banished Cells I",["repeatType"]="0",["displayType"]="2",["backgroundText"]="The Banished Cells have been overrun by Daedra. They have set free the immortal High Kinlord Rilis, an evil being of great magical power. I have agreed to help Keeper Cirion, the warden of the Banished Cells, deal with this crisis.",["poiIndex"]="41",["isShareable"]="1"},
{["internalId"]="3688",["name"]="Unwelcome Guests",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Doubts-the-Moon says some mercenaries in Stormhold are stirring up trouble between the Dark Elves and Argonians.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3645",["name"]="The Sapling",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="Ebonheart",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Zasha-Ja fears for the survival of the Hist Tree the Argonians transplated from Black Marsh to Ebonheart. He thinks the Dark Elves of House Dres poisoned it, so he wants to fertilize it to encourage its growth.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="3667",["name"]="Night of the Soul",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="Ebonheart",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Dunmer of Ebonheart have turned to Disciple Sildras for comfort in the face of an expected assault by Covenant forces. Disciple Sildras, however, believes the Tribunal has rejected the town. His gloom is affecting the citizens' morale.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="3639",["name"]="Restoring Order",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="Ebonheart",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A strong Covenant raiding force landed west of Ebonheart. The citizens are too busy with their own internal squabbles to unite against invaders. Drathus Othral fears the town may fall to Alexandra Conele's forces.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="3690",["name"]="Scouring the Mire",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vicecanon Hrondar suspects the Dominion skin-stealers are based out of Ten-Maur-Wolk.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4587",["name"]="Trail of the Skin-Stealer",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Ten-Maur-Wolk",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The vicecanons of Shadowfen tracked the Dominion's forces to the ancient Daedric ruin of Ten-Maur-Wolk.",["poiIndex"]="8",["isShareable"]="1"},
{["internalId"]="4590",["name"]="The Skin-Stealer's Lair",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Ten-Maur-Wolk",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vicecanon Hrondar was replaced by a Dominion skin-stealer. I need to find the imposter and stop the remaining skin-stealers in Ten-Maur-Wolk.",["poiIndex"]="8",["isShareable"]="1"},
{["internalId"]="5499",["name"]="Wrecked",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I discovered a merchant's permit lying unattended. The permit mentions three trading partners from Daggerfall seeking to establish a shop in Orsinium.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5320",["name"]="The Durzog Whistle",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a whistle in a backpack in a cave full of durzog. I should try to use the whistle on the durzogs and then see if anyone in Orsinium is interested in the thing.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4259",["name"]="Foreign Vintage",["level"]="34",["type"]="5",["zone"]="Shadowfen",["objective"]="Sanguine's Demesne",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Spies from the Aldmeri Dominion have infiltrated Sanguine's celebration. Seducer Trilvath gave me Sanguine's goblet and wants me to find the spies.",["poiIndex"]="40",["isShareable"]="1"},
{["internalId"]="4237",["name"]="Life of the Party",["level"]="34",["type"]="5",["zone"]="Shadowfen",["objective"]="Sanguine's Demesne",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Sings-with-Drink has been trapped in Sanguine's celebration for four years. He's convinced he can only escape if the other party goers are distracted by an entertainment troupe from Morrowind.",["poiIndex"]="40",["isShareable"]="1"},
{["internalId"]="4607",["name"]="Castle of the Worm",["level"]="50",["type"]="2",["zone"]="Castle of the Worm",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="I must find a way into the Castle of the Worm. There will be many dangers ahead, so I must make sure I am prepared.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4836",["name"]="Halls of Torment",["level"]="50",["type"]="2",["zone"]="The Halls of Torment",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="Varen Aquilarios and Abnur Tharn have found the location of Sai Sahan within the Halls of Torment. I need to venture into these halls and save the final, missing companion.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4837",["name"]="Valley of Blades",["level"]="50",["type"]="2",["zone"]="The Valley of Blades",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="Varen Aquilarios asked me to accompany Sai Sahan on a trip back to his ancestral home. An old friend, Kasura, may have knowledge of how to sneak into Sancre Tor.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5836",["name"]="Bound by Love",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Llonas Givyn desires to marry his love, Tirwin. Unfortunately, Tirwin is a slave belonging to Llonas's mother, Faras, who disapproves of the pairing.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6131",["name"]="Storming the Walls",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="Sea Keep",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A massive storm is brewing over the fortress of Sea Keep that is the work of a Maormer Sea Mage. It will devastate the entire coast unless the Sea Mage's spell is stopped.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="5443",["name"]="Thukhozod the Eternal",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met a pair of adventurers outside a ruin who offered me a share of their gold if I help them complete a job. They've been hired to find a missing young Orc inside the ruins known as Thukhozod's Sanctum.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4548",["name"]="Farsight",["level"]="11",["type"]="0",["zone"]="Betnikh",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="An Orc named Magula has seen visions at three shrines around the island. I should visit these shrines and see the visions.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4559",["name"]="Daughter of Seamount",["level"]="13",["type"]="0",["zone"]="Betnikh",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The hunt-wife of the Seamount clan thinks the chief's daughter is not mindful of her duties to the clan. She believes I could help sort things out.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5538",["name"]="Voices in the Dark",["level"]="8",["type"]="3",["zone"]="Gold Coast",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Rumors abound that the Dark Brotherhood, assassins of legend, are on the lookout for members worthy to wield their killing blade. This might be why so many innocents have been murdered of late.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3914",["name"]="Missing Son",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Tah-Tehat asked me to find his son who hasn't returned to the village.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3608",["name"]="The Medallions of Saint Veloth",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Muth Gnaar",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Healer Ravel asked me to collect Saint Veloth medallions. As long as the monks are allowed to pray in peace, the medallions allow the power of the Tear of Saint Veloth to keep the Llodos plague in check on whoever wears one.",["poiIndex"]="3",["isShareable"]="1"},
{["internalId"]="3611",["name"]="Burning Revenge",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Muth Gnaar",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Relnus Andalen and his brothers want to take revenge on the Vereansu for killing their father, who was afflicted with the Llodos plague and was being cared for by the monks of Muth Gnaar.",["poiIndex"]="3",["isShareable"]="1"},
{["internalId"]="3605",["name"]="Ritual of Anguish",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Malak's Maw",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I encountered a tormented Dark Elf spirit in the Orc camp at Malak's Maw. She asked for aid in easing her torment.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="3600",["name"]="Oath Breaker",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Malak's Maw",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I encountered a Dunmer lurking in a cave near Malak's Maw. He informed me that a Nord woman named Svanhildr was captured by Orcs. I offered to rescue her in his stead.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="3604",["name"]="Challenge the Tide",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Malak's Maw",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Svanhildr wants to break the Oathbound offensive once and for all.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="3593",["name"]="Intruders in Deshaan",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Malak's Maw",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Dark Elves have been disappearing along the road to Narsis, near the western mountains. An incursion of Orcs may be responsible. I offered a Dark Elf scout my assistance.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="4455",["name"]="Trade Negotiations",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Vale of the Ghost Snake",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Merchants from House Hlaalu are worried about their leader, Raston Vendil. He went to negotiate a trade agreement with the isolated Mabrigash tribe in the Vale of the Ghost Snake. He never returned.",["poiIndex"]="9",["isShareable"]="1"},
{["internalId"]="3651",["name"]="The Trial of the Ghost Snake",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Vale of the Ghost Snake",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Farseer Bodani has placed the fate of Raston Vendil in my hands. I agreed to undertake a trial and see if the Ghost Snake will counsel me on this matter.",["poiIndex"]="9",["isShareable"]="1"},
{["internalId"]="4424",["name"]="A Debt Come Due",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Sylvian Herius wants an Imperial lieutentant killed, to hurt their army's morale and readiness.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4530",["name"]="Goblin's Delight",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I agreed to help Aemilia Hadrianus kill a Goblin that Chef Amadour has been feeding.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4674",["name"]="Spoils of War",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Someone named Jena is being threatened. She apparently lives in Cheydinhal.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4691",["name"]="Cause and Effect",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a letter from someone named Iocundus in Cheydinhal.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3820",["name"]="Restless Spirits",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Selfora",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Some terrible catastrophe has befallen Selfora, and now hostile spirits overrun the place.",["poiIndex"]="19",["isShareable"]="1"},
{["internalId"]="3904",["name"]="Supply Run",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Aamela Rethandus needs healing supplies to treat Selfora's wounded.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3852",["name"]="Rescue and Revenge",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Selfora",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Forlorn One asked me to use the decanter of holy water to rescue two people trapped by the fires.",["poiIndex"]="19",["isShareable"]="1"},
{["internalId"]="3620",["name"]="The Ravaged Village",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Silent Mire",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I encountered Vara-Zeen, the leader of the Argonian village of Silent Mire. The settlement is under attack by Khajiiti pirates and I have offered my assistance.",["poiIndex"]="2",["isShareable"]="1"},
{["internalId"]="3650",["name"]="Cold-Blooded Vengeance",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Silent Mire",["repeatType"]="0",["displayType"]="0",["backgroundText"]="An extremely angry Argonian named Azeenus has asked me to seek vengeance against the Khajiiti pirates in his name.",["poiIndex"]="2",["isShareable"]="1"},
{["internalId"]="3624",["name"]="The Saving of Silent Mire",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Silent Mire",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Leel-Vata asked me to rescue the captured Argonian villagers from the Khajiiti pirates.",["poiIndex"]="2",["isShareable"]="1"},
{["internalId"]="3715",["name"]="Strange Guard Beasts",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Churasu has a way to turn the giant wasps into an army to use against the Khajiiti pirates. She needs my help to make an alchemical concoction.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3728",["name"]="Remembering Risa",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a battered journal while walking the Coiled Path. A young woman named Risa did not survive this place. She lost her amulet fighting one of the snakes.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3684",["name"]="Bad Soldiers",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Tal'Deic Fortress",["repeatType"]="0",["displayType"]="0",["backgroundText"]="It appears that soldiers known as Sentinels are terrorizing the fortress.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="3666",["name"]="Rules and Regulations",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Tal'Deic Fortress",["repeatType"]="0",["displayType"]="0",["backgroundText"]="General Gavryn Redoran is acting erratically and completely out of character. Something has changed. I get the impression that Tal'Deic Fortress is in some kind of trouble.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="3702",["name"]="The Soldier's Alibi",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Tal'Deic Fortress",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I encountered a captive Dark Elf soldier named Saryvn in the ruins beneath Tal'Deic Fortress. He was captured by the Maulborn while he was avoiding his duties.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="3683",["name"]="What Lies Beneath",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Tal'Deic Fortress",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I volunteered to enter the ruined crypts beneath the fort to look for the retainer, Durel Gilveni, and the mysterious others mentioned in the General's dispatch. The entrance is by the river.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="4867",["name"]="Shadow of Sancre Tor",["level"]="50",["type"]="2",["zone"]="Sancre Tor",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="Sai Sahan has recovered from his experiences in the Halls of Torment and is ready to guide me to the Amulet of Kings. He hid the artifact in Sancre Tor, an ancient Imperial catacomb.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3749",["name"]="Into the Mouth of Madness",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Eidolon's Hollow",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Blessed by Veloth, I've come to Eidolon's Hollow to confront Magistrix Vox. It's time to end her threat once and for all.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="3810",["name"]="Motive for Heresy",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Eidolon's Hollow",["repeatType"]="0",["displayType"]="10",["backgroundText"]="With Vox's wards shattered, the path to her lair is open. Aspera the Forgotten suggests this creates an opportunity to defeat Vox, but I'm not sure I can trust this strange projection.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="3831",["name"]="The Judgment of Veloth",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Eidolon's Hollow",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The time to confront Vox has arrivedâ€”but it seems that to defeat her, I need to die by her hammer. The Blessing of Veloth should protect me, but can I return from the dead?",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="3893",["name"]="By Invitation Only",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Shad Astula",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Deshaan's Academy of Magic, Shad Astula, trains only the most promising mages to become leaders of the Ebonheart Pact. However, the Academy only accepts those who've received a special invitation.",["poiIndex"]="18",["isShareable"]="1"},
{["internalId"]="6399",["name"]="Order of the New Moon",["level"]="50",["type"]="0",["zone"]="Southern Elsweyr",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Sai Sahan wants me to infiltrate the Dragon cult and learn more about them.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4565",["name"]="Do as I Say",["level"]="50",["type"]="0",["zone"]="Stormhaven",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I've found a scared bandit trying to hide in Portdun Watch. He wants me to take a token to his brother, though he was too frightened to tell me much more.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4519",["name"]="Word from the Dead",["level"]="50",["type"]="0",["zone"]="Stormhaven",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A mortally wounded man in Norvulk Ruins asked me to take word of his death to his wife.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="1637",["name"]="Divert and Deliver",["level"]="50",["type"]="0",["zone"]="Stormhaven",["objective"]="Steelheart Moorings",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Slavers are using Steelheart Moorings as their base of operations and they're holding some sailors who they recently captured. The first mate has asked me to help free the sailors from the slavers' clutches.",["poiIndex"]="12",["isShareable"]="1"},
{["internalId"]="4589",["name"]="Jumping Ship",["level"]="50",["type"]="5",["zone"]="Bangkorai",["objective"]="Dungeon: Blackheart Haven",["repeatType"]="0",["displayType"]="2",["backgroundText"]="Shifty Tom, one of Captain Blackheart's underlings, wants to give up the pirate's life. He plans to leave Blackheart Haven, the Captain's island fortress, but wants me to help him steal two magical relics from the Captain, first.",["poiIndex"]="36",["isShareable"]="1"},
{["internalId"]="3815",["name"]="Cracking the Egg",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vicecanon Servyna wants to find out more about the Mnemic Egg. Until the vicecanons understand its danger, they can't root out the Dominion in Shadowfen.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4606",["name"]="Keepers of the Shell",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Hatching Pools",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vicecanon Servyna asked me to help Keeper Uxith-Ei in the Hatching Pools. Dominion forces overran the area.",["poiIndex"]="13",["isShareable"]="1"},
{["internalId"]="4646",["name"]="The Mnemic Egg",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Dominion took the Mnemic Egg to the Ayleid ruin of Loriasel. There they used its power to sever the Argonian connection with the Hist. As long as it remains in their possession, no Argonian eggs can hatch.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3911",["name"]="The Swamp's Embrace",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Maahi is one of the few surviving prisoners from the Dominion's march into Loriasel.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3912",["name"]="Vigil's End",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="After picking up an ancient sword in Loriasel, I learned an Ayleid knight's spirit haunts the ruins.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3909",["name"]="The Dominion's Alchemist",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Loriasel",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vicecanon Heita-Meen tracked the Dominion's movement to Loriasel, where she hopes to end the Dominion invasion of Shadowfen.",["poiIndex"]="32",["isShareable"]="1"},
{["internalId"]="3910",["name"]="The Dream of the Hist",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Loriasel",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Vicecanon Heita-Meen asked me to retreive the Mnemic Egg from Ruuvitar, the architect of the Dominion invasion in Shadowfen.",["poiIndex"]="32",["isShareable"]="1"},
{["internalId"]="3674",["name"]="Warm Welcome",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Alten Corimont",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Some pirates in Alten Corimont captured one of the vicecanons' assistants from Stormhold.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="3675",["name"]="Last One Standing",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Alten Corimont",["repeatType"]="0",["displayType"]="0",["backgroundText"]="One-Eye wants to get rid of Tsanji, the Khajiit in Alten Corimont. Since the Khajiit is running Dominion supply lines, her sudden demise would benefit both One-Eye and the rest of Shadowfen.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="3676",["name"]="A Pirate Parley",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Alten Corimont",["repeatType"]="0",["displayType"]="0",["backgroundText"]="S'jash has a plan to steal treasure from Dominion troops before they find out what happened to Tsanji, their contact in Alten Corimont.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="4474",["name"]="Daughter of Giants",["level"]="12",["type"]="2",["zone"]="The Foundry of Woe",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="The Prophet has contacted me across great distance for some important purpose.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4552",["name"]="Chasing Shadows",["level"]="13",["type"]="2",["zone"]="The Harborage",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="The Prophet and I are seeking a way to rescue Sai Sahan, the Redguard who hid away the Amulet of Kings.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="6119",["name"]="The Ebon Sanctum",["level"]="14",["type"]="0",["zone"]="Summerset",["objective"]="Ebon Stadmont",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I agreed to escort the scholar Andewen as she investigates an ancient temple, hidden within the mysterious forest of Ebon Stadmont.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="2759",["name"]="Kill Enemy Players",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Grand Warlord Dortene has authorized a bonus for any Covenant soldier who slays 20 enemy combatants.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3724",["name"]="Lost to the Mire",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Luteema went to the Percolating Mire. He has yet to reply to Rabeen-Ei's correspondence.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3900",["name"]="Into the Mire",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Percolating Mire",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Dominion troops invaded Feyne Vildan's village, trapping her husband and the other villagers. Whatever the soldiers are doing in Percolating Mire, they must be stopped.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="3905",["name"]="Clarity",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Percolating Mire",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Feyne Vildan's husband Ja-Reet won't speak since his ordeal with the Dominion. I must find a way to heal him.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="3888",["name"]="Buried in the Past",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Sunscale Strand",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Researchers seeking ancient relics around the xanmeers found Renrijra pirates camped in the Argonian ruins they were planning to search.",["poiIndex"]="14",["isShareable"]="1"},
{["internalId"]="3889",["name"]="The Fangs of Sithis",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Sunscale Strand",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The artifacts buried beneath Sunscale Strand are dangerous. Looks-Under-Rocks warns that we may have trouble with them. I need to be careful when searching the ruins.",["poiIndex"]="14",["isShareable"]="1"},
{["internalId"]="3890",["name"]="Pull the Last Fang",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Sunscale Strand",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I left the ancient Argonian ruins with a single Fang of Sithis. According to Looks-Under-Rocks, the Renrijra pirates are in an uproar. They may be feeling the effects of the other Fang in their midst.",["poiIndex"]="14",["isShareable"]="1"},
{["internalId"]="3730",["name"]="Whispers of the Wisps",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Deep Graves",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Deep Graves is plagued by wisps. They're stealing away travelers at the gravesite. No one is sure what caused their sudden aggression.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="3729",["name"]="A Stranger Uninvited",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Deep Graves",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Xijai-Teel welcomed a traveling High Elf to Deep Graves, but she believes he has something to do with the wisps' recent aggression.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="3731",["name"]="Broken Apart",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Deep Graves",["repeatType"]="0",["displayType"]="0",["backgroundText"]="To rid the area of her corrupting influence, Xijai-Teel and I cleansed the ancient vessel. We need it capture the wispmother in Deep Graves.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="3799",["name"]="Scales of Retribution",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Murkwater",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Dominion established a foothold in the small town of Murkwater. The Argonian Shadowscales intend to take back the town by force.",["poiIndex"]="9",["isShareable"]="1"},
{["internalId"]="4831",["name"]="The Harborage",["level"]="4",["type"]="2",["zone"]="The Harborage",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="The Prophet contacted me through a magical projection. He asked me to seek out a place called the Harborage, where he has taken refuge. I should go to him and ensure that he is safe.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3590",["name"]="Through the Aftermath",["level"]="4",["type"]="0",["zone"]="Stonefalls",["objective"]="Starved Plain",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The remnants of a broken siege lie outside Davon's Watch, broken by the appearance of an army of ghosts. A sergeant at the front, a Nord named Rhorlak, has asked me to decide how best to deal with the spirits.",["poiIndex"]="3",["isShareable"]="1"},
{["internalId"]="3594",["name"]="Enslaved in Death",["level"]="4",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Even though they're dead, Ix-Utha's Argonian brethren still act as slaves to the risen Chimer. They've returned to serve their cruel spectral masters.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6165",["name"]="The Abyssal Cabal",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A Battlereeve sent a group of adventurers and a squad of marines to determine the extent of the Sea Sload's incursion into Summerset. After an initial report, the Battlereeve lost contact with the group. She asked me to investigate.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3804",["name"]="Missing in the Mire",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Xal Ithix",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Argonians in Xal Ithix have been picking through trash piles. Now normally docile creatures from the swamp are attacking them.",["poiIndex"]="6",["isShareable"]="1"},
{["internalId"]="3795",["name"]="Deep Disturbance",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Xal Ithix",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Something caused the creatures in Xal Ithix's swamps to become aggressive and attack the villagers.",["poiIndex"]="6",["isShareable"]="1"},
{["internalId"]="3845",["name"]="And Throw Away The Key",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="White Rose Prison",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The vicecanons tracked an ancient keystone to White Rose Prison. This artifact would be a major boon to Pact forces in Shadowfen. However, the prisoners seem unwilling to negotiate directly with the vicecanons.",["poiIndex"]="25",["isShareable"]="1"},
{["internalId"]="3847",["name"]="The Ones Left Behind",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Erranza, a former prisoner of White Rose Prison, is dead, but her grieving spirit lingers on.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3846",["name"]="The Keystone",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="White Rose Prison",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The keystone is located within the White Rose Prison Dungeon. I've agreed to find it and use it to free the prisoners here.",["poiIndex"]="25",["isShareable"]="1"},
{["internalId"]="4532",["name"]="Take Me Home",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Deep inside a dungeon called Mehrunes' Spite, I found a scamp with a labeled choker. It apparently belongs to a mage in Skywatch.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2860",["name"]="Scout Glademist Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Glademist Mine for the Pact.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="6381",["name"]="A Dastardly Duo",["level"]="50",["type"]="1",["zone"]="Northern Elsweyr",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="According to Ri'hirr, a pair of Khajiiti criminals named Akumjhargo and Zav'i bleed Northern Elsweyr dry through a combination of theft, murder, and extortion. Honest trade cannot flourish as long as these lawbreakers remain alive.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5037",["name"]="Stem the Tide",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Covenant soldiers are massing within Softloam Cavern. It won't be long before they have the numbers to launch an attack on Kragenmoor and the rest of Stonefalls.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4435",["name"]="Simply Misplaced",["level"]="50",["type"]="3",["zone"]="Vulkhel Guard",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="My first trial in Sheogorath's realm awaits. Valaste and Shalidor have uncovered what they need to get me where I need to go.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="6094",["name"]="The Broken Brassilisk",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found an inactive brassilisk in the Radius of the Clockwork City with the name \"Frynga\" carved into its jaw. If I find Frynga and return her mechanical pet, perhaps she will reward me.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6076",["name"]="Inciting the Imperfect",["level"]="50",["type"]="1",["zone"]="Clockwork City",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="I've accepted a contract to defeat Sotha Sil's creation, the Imperfect, in order to test its capabilities. I've also been asked to gather its lost sequence plaques.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6060",["name"]="Tasty Tongue Varnish",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Brengolin, the cook in Slag Town, needs some assistance in creating some unconventional cuisine. This time he's asked me to procure him some Bosmer Root Liqueur.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6061",["name"]="A Matter of Tenderness",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Brengolin, the cook in Slag Town, needs some assistance in creating some unconventional cuisine. His meat stores are presently empty, so I agreed to assist him with restocking.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6074",["name"]="Cogs of Fate",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The people of Slag Town apparently seek predictions about the future from a factotum known as the Grand Prognosticator. Kamid, whose mother died during the last pilgrimage, plans to prove that the oracle is a fraud.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6093",["name"]="The Mage's Dog",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found the journal of Orinol, a dead mage from the city of Cormount, in Grahtwood. He arrived in Clockwork City by accident and died in the Radius sometime later. His dying wish was that someone would care for his dog, Jona.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3865",["name"]="Savages of Stonefalls",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The wilderness of Stonefalls is filled with predators and hostile creatures. The citizens of the region would sleep better knowing the worst ones are dead.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3916",["name"]="Long Lost Lore",["level"]="6",["type"]="3",["zone"]="Davon's Watch",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="A high-ranking member of the Mages Guild, Valaste wants me to perform a task for her.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3587",["name"]="Delaying the Daggers",["level"]="12",["type"]="0",["zone"]="Stonefalls",["objective"]="Davon's Watch",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Tanval Indoril asked me to help slow the Covenant's siege of Davon's Watch.",["poiIndex"]="4",["isShareable"]="1"},
{["internalId"]="3588",["name"]="City Under Siege",["level"]="12",["type"]="0",["zone"]="Stonefalls",["objective"]="Davon's Watch",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Covenant forces have scaled the walls of Davon's Watch. Now they're threatening to take the city from within its walls. Tanval's ritual may be our only hope to end the assault.",["poiIndex"]="4",["isShareable"]="1"},
{["internalId"]="6299",["name"]="The Demon Weapon",["level"]="6",["type"]="14",["zone"]="Grahtwood",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Imperial battlemage and former Grand Chancellor of the Empire, Abnur Tharn, requested my assistance with an important matter that could lead to the end of the Three Banners War.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6395",["name"]="The Dragonguard's Legacy",["level"]="6",["type"]="14",["zone"]="The Valley of Blades",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Blademaster Kasura has agreed to search a nearby Dragonguard tomb for a former student interested in finding some way to fight back against the Dragons in Elsweyr. I agreed to help her in this endeavor.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6097",["name"]="Through a Veil Darkly",["level"]="6",["type"]="14",["zone"]="Glenumbra",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met Vanus Galerion in the Mages Guild. Turns out, he had a message for me from an Oracle. It must be important for the Great Mage to deliver it personally.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3592",["name"]="Proving Trust",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Senie",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A volcanic eruption and an infestation of shalks have devastated the town of Senie. The townsfolk are wounded, but distrust between Dark Elves and Argonians delay efforts to heal them.",["poiIndex"]="12",["isShareable"]="1"},
{["internalId"]="3602",["name"]="Percussive Ranching",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Drelden Orn lost most of his kwama in a mine collapse. He asked me to help round up the survivors and convince them to return home.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3589",["name"]="Quiet the Ringing Bell",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="With the Covenant driven from Davon's Watch, Tanval Indoril has asked me to return the living weapon known as Balreth to its cell.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3855",["name"]="Mystery of Othrenis",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Garyn Indoril believes the spirit of a long-dead mage may know the secret to imprisoning Balreth again. An Argonian scout named Onuja has already left for Othrenis.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3615",["name"]="Wake the Dead",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Othrenis",["repeatType"]="0",["displayType"]="10",["backgroundText"]="An Argonian named Onuja has come to Othrenis. He wants to know how to rebind the Brother of Strife. He's hoping to contact the spirit of Mavos Siloreth, an ancient Dunmer scribe buried in the area.",["poiIndex"]="6",["isShareable"]="1"},
{["internalId"]="3616",["name"]="Rending Flames",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Othrenis",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I awakened the spirit of an ancient Dark Elf scribe named Mavos Siloreth. Mavos's spirit can't remember how he bound Balreth in the original ceremony. To help me, he's going to perform a ritual and show me a vision of the past.",["poiIndex"]="6",["isShareable"]="1"},
{["internalId"]="5876",["name"]="A Dangerous Breed",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A mad breedsman named Drovos Nelvayn has created a dangerous new genus of kwama. These insects attack without provocation and resist any attempts at domestication. If the creatures escape, native kwama and the miners that tend them will suffer.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3617",["name"]="Quieting a Heart",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Bala, an Ashlander, is searching for her deceased lover, Eanen Varam. In life, he was part of House Dunmer, so their status kept them apart. They promised each other to unite in death, but now, she can't find his tomb.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3618",["name"]="To Ash Mountain",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've learned all I can at Othrenis. Onuja says that Balreth is somewhere near Ash Mountain. Garyn Indoril has led many Pact soldiers there with hopes of containing the Brother of Strife.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3736",["name"]="Close the Scamp Caves",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Scamps are boiling up out of the ground, no doubt excited by the freeing of Balreth. Sergeant Jagyr asked for my help dealing with them. Collapsing the entrances to caves in Ash Mountain should do the job.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3734",["name"]="Restoring the Guardians",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="Ash Mountain",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Garyn Indoril led a force of Pact soldiers to Ash Mountain. They were following the Brother of Strife, Balreth. Using the information I learned from Mavos at Othrenis, we're going to imprison the Brother of Strife again.",["poiIndex"]="17",["isShareable"]="1"},
{["internalId"]="3735",["name"]="The Death of Balreth",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="Ash Mountain",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've defeated Ash Mountain's guardians. Now I must enter Ash Mountain's caldera, defeat Balreth, and imprison him again.",["poiIndex"]="17",["isShareable"]="1"},
{["internalId"]="3670",["name"]="Desperate Souls",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Rogue Dark Elves flaunting the Pact are enslaving Argonians at a mine near Ash Mountain. A powerful Daedra rebelled against the mine's master. Then the Daedra took most of the people at the mine into Oblivion.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6070",["name"]="Nibbles and Bits",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Bursar of Tributes seeks items of flesh to add to the Blackfeather Court's hoard.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3717",["name"]="King of Dust",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Zuuk",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Dominion is searching for a keystone in Zuuk. I need to find it before they do.",["poiIndex"]="4",["isShareable"]="1"},
{["internalId"]="3718",["name"]="Into the Temple",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Zuuk",["repeatType"]="0",["displayType"]="0",["backgroundText"]="After Drillk's memory returned, he told me how to obtain the keystone. It's locked away in the Temple of Sul.",["poiIndex"]="4",["isShareable"]="1"},
{["internalId"]="3719",["name"]="Captured Time",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I've found a book about Ayleid 'Alasil pilasters' within the Temple of Sul. Beside it was a small Ayleid relic that may be able to awaken the pilasters.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3681",["name"]="Lost Pilgrimage",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A group of pilgrims failed to arrive in Hissmir as scheduled. Kiameed fears something has happened to them.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6036",["name"]="Most Complicated Machine",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Proctor Sovor Saryoni, a ranking Clockwork Apostle, is dying and has asked me to help save his life. He believes his only hope to continue his existence lies in an experiment conducted by a former apostle, but she was exiled long ago because of it.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2799",["name"]="Scout Alessia Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Alessia Mine for the Covenant.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5904",["name"]="Salothan's Curse",["level"]="50",["type"]="1",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="I've been hired to defeat the spirits which haunt Salothan Tomb.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5934",["name"]="Tax Deduction",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="I have been asked to retrieve taxes and records stolen by bandits.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5956",["name"]="Daedric Disruptions",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Sunken Vaults of Ashalmawia are infested with Worm Cultists performing unclean rites to summon Daedra.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5866",["name"]="Oxen Free",["level"]="50",["type"]="1",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="I've been hired to defeat a nix-ox that has been eating mushrooms the wizard Bravosi Felder is attempting to research. I'm also required to collect samples of the mushrooms while in the area.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4309",["name"]="Eye Spy",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="North Beacon",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met Arfanel, a member of the Eyes of the Queen at North Beacon. He's tracking an elite Covenant soldier and wants my assistance supporting the troops in the field.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="3227",["name"]="Capture Castle Brindle",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Castle Brindle for the Covenant.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4295",["name"]="The Wayward Dagger",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="North Beacon",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A Covenant officer and her husband attempted to defect to the Aldmeri Dominion. Covenant troops pursued the Orc couple all the way to Auridon, and they've taken North Beacon in an attempt to capture the Orcs.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="4361",["name"]="Rightful Inheritance",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="Castle Rilis",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Elanwe's son, Sorondil, is the illegitimate son of the deceased High Kinsman Rilis XII. Sorondil has come to these ruins to retrieve a great treasure and his inheritance.",["poiIndex"]="35",["isShareable"]="1"},
{["internalId"]="6166",["name"]="Sinking Summerset",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Battlereeve Tanerline charged me with finding allies to attack the abyssal geysers and destroy the pearls that power them before the Sea Sload cabal's plan to sink Summerset comes to fruition.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6073",["name"]="A Shadow Misplaced",["level"]="50",["type"]="1",["zone"]="Clockwork City",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Clockwork Apostles have sent explorers into the Shadow Cleft to find out more about the mysterious realm and the reason it's connected to Clockwork City. One explorer hasn't returned, and I agreed to try to find him.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6240",["name"]="The Lost Legion",["level"]="50",["type"]="0",["zone"]="Murkmire",["objective"]="Tsofeer Cavern",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Dradeiva, a local Argonian, seeks to prove that the Imperial Ninth Legion disappeared in the Tsofeer Caverns. She hopes to show that her ancestor didn't betray Murkmire. Unfortunately, the caves may also be home to the monstrous voriplasm, Wuju-Ka.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="3583",["name"]="Suspicious Silence",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="Fort Arand",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Covenant troops are massing to the north of Fort Arand. They don't have the numbers for a frontal assault, but Fort Captain Hjolm believes they may be coordinating an attack with a second flank.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="2920",["name"]="Capture Fort Dragonclaw",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Fort Dragonclaw for the Dominion.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2673",["name"]="Scout Fort Rayles",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Fort Rayles for the Dominion.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="6239",["name"]="She Who Eats the Light",["level"]="50",["type"]="0",["zone"]="Murkmire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Teeth of Sithis, a pair of ancient xanmeer temples, has been overtaken by the servants of its heretical, former high priestess, Shuxaltsei. She has taken the custodians of the Teeth captive and is sacrificing them in some profane blood rite.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3591",["name"]="The Venom of Ahknara",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="Fort Arand",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Covenant troops have killed Captain Hjolm of Fort Arand, and they're threatening the safety of the recruits within the fort. Captain Llaari hatched a plan to respond to the threat.",["poiIndex"]="1",["isShareable"]="1"},
{["internalId"]="3627",["name"]="Kinsman's Revenge",["level"]="50",["type"]="0",["zone"]="Stonefalls",["objective"]="Hrogar's Hold",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Gar has been taken captive by the Goblins, and Fafnyr was wounded in the fighting. The Nord warrior seeks revenge against the hated Goblins and their spider allies.",["poiIndex"]="5",["isShareable"]="1"},
{["internalId"]="4844",["name"]="The Price of Longevity",["level"]="50",["type"]="0",["zone"]="Rivenspire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a strange sapling that spoke to me, confused about its own ability to think and feel. It has vague memories of walking and a nearby cave. I agreed to help the sapling get some answers.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4135",["name"]="Pulled Under",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="Riften",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The people of Riften have been brought inside their shattered walls. While Sinmur is gone, however, Riften's people may face other threats.",["poiIndex"]="21",["isShareable"]="1"},
{["internalId"]="4304",["name"]="Redguard on the Run",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Gorgath Deadeye is after a Redguard called Varnedo. Rumor has Varnedo in Ivarstead and Gorgath will give me a reward for tracking him down.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6129",["name"]="The Perils of Art",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Tindoria, a High Elf artist, has hired me to acquire a list of rare art supplies for her next masterpiece.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4638",["name"]="The Real Snake",["level"]="50",["type"]="0",["zone"]="Alik'r Desert",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I've found a letter indicating a man named Fadeel is tricking people into traveling to Sandblown Mine to provide \"fresh meat\" for the snake, Huzal.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3903",["name"]="School Daze",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Shad Astula",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Arch-Mage Valeyn believes I'm here to help protect the Academy against some unnamed threat. While he determines what this threat is, he asked me to help him with another matter.",["poiIndex"]="18",["isShareable"]="1"},
{["internalId"]="3908",["name"]="Vision Quest",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="Shad Astula",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Arch-Mage Valeyn thinks his visions are somehow connected to the evil Magistrix Vox. Unfortunately, before he could learn more, something blocked his magic.",["poiIndex"]="18",["isShareable"]="1"},
{["internalId"]="4493",["name"]="A Fair Warning",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A note posted at the entrance to Knife Ear Grotto by a scholar warns of danger inside.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3809",["name"]="Cultural Exchange",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Rigurt the Brash, leader of the Expedition for Nord Cultural Exchange, has asked for my assistance. I've agreed to trade a Nord book, clothes, and relic for similar Dark Elf items.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4163",["name"]="Onward to Shadowfen",["level"]="50",["type"]="0",["zone"]="Deshaan",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Ganthis, an agent of Shadowfen's leadership, asked me to help Vicecanon Hrondar investigate a number of murders in Stormhold.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3840",["name"]="Saving the Relics",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Bogmother",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Shadowfen's Vicecanon Hrondar sent Saervild Steel-Wind to Bogmother's ruins to collect Argonian relics.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="3828",["name"]="Cold-Blooded Revenge",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Dominion slaughtered the Argonians living in a small hamlet in Shadowfen. Only one remains alive to take his vengeance.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3827",["name"]="The Tree-Minder's Fate",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Forsaken Hamlet",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Mystery surrounds Forsaken Hamlet. An Argonian tree-minder lies murdered at the edge of her village.",["poiIndex"]="12",["isShareable"]="1"},
{["internalId"]="4553",["name"]="Threefold Folly",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Erranenen lost his lute in a riddle game with Akishan.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3585",["name"]="Legacy of the Ancestors",["level"]="7",["type"]="0",["zone"]="Stonefalls",["objective"]="Davon's Watch",["repeatType"]="0",["displayType"]="10",["backgroundText"]="A Nord commander named Holgunn asked me to help defend Davon's Watch.",["poiIndex"]="4",["isShareable"]="1"},
{["internalId"]="6403",["name"]="The Pride of Alkosh",["level"]="50",["type"]="0",["zone"]="Southern Elsweyr",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Order of the New Moon plans to attack Pridehome, a reclusive temple dedicated to the worship of Alkosh. The Dragonguard is determined to stop the cult's assault and uncover the motives behind the attack.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6436",["name"]="Lilies for Remembrance",["level"]="50",["type"]="0",["zone"]="Southern Elsweyr",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Senchal job broker, Guybert Flaubert, has a client who wants some lilies planted in Moonlit Cove as a tribute to her late grandmother.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6423",["name"]="Dust Smote",["level"]="50",["type"]="1",["zone"]="Southern Elsweyr",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="A territorial guardian watches over a temple of Meridia in Southern Elsweyr. I agreed to help kill the guardian and gather some temple dust.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6186",["name"]="Moonlight Ascent",["level"]="50",["type"]="5",["zone"]="Reaper's March",["objective"]="Moon Hunter Keep",["repeatType"]="0",["displayType"]="2",["backgroundText"]="Adonatus Varian, member of the Silver Dawn, has tasked me with taking down the werewolf pack within Moon Hunter Keep. I'll need to work my way through the keep in order to reach the pack's leader, Vykosa.",["poiIndex"]="62",["isShareable"]="1"},
{["internalId"]="6187",["name"]="Pledge: Moon Hunter Keep",["level"]="50",["type"]="15",["zone"]="Reaper's March",["objective"]="Moon Hunter Keep",["repeatType"]="2",["displayType"]="2",["backgroundText"]="Urgarlag Chief-bane of the Undaunted has challenged me to explore Moon Hunter Keep.",["poiIndex"]="62",["isShareable"]="0"},
{["internalId"]="4597",["name"]="The Plan",["level"]="50",["type"]="5",["zone"]="Auridon",["objective"]="Dungeon: The Banished Cells II",["repeatType"]="0",["displayType"]="2",["backgroundText"]="The Ancient Keepers, sworn to imprison the spirit of High Kinlord Rilis, have taken to forming Daedric pacts. Keeper Cirion has asked for help in defeating the corrupting force of Rilis, but will require fighting the Keepers directly.",["poiIndex"]="61",["isShareable"]="1"},
{["internalId"]="5014",["name"]="Crimes of the Past",["level"]="50",["type"]="0",["zone"]="Rivenspire",["objective"]="Sanguine Barrows",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Ring of Daggers has tracked down a war criminal named Gerard Althen, who once served as King Ranser's spymaster. He's hiding somewhere in Sanguine Barrows and I've agreed to find him and bring him out.",["poiIndex"]="9",["isShareable"]="1"},
{["internalId"]="4955",["name"]="A Lucky Break",["level"]="50",["type"]="0",["zone"]="Rivenspire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a journal that belonged to a thief called Nedras the Sly. He apparently turned to crime in order to support his ailing sister in Northpoint.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4524",["name"]="Repentant Son",["level"]="50",["type"]="0",["zone"]="Auridon",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found the journal of a young man named Vareldur in the mine called Del's Claim. He died trying to stop the Heritance from killing the miners there.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3085",["name"]="Capture Fort Glademist",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Fort Glademist for the Pact.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3856",["name"]="Anchors from the Harbour",["level"]="8",["type"]="3",["zone"]="Davon's Watch",["objective"]="",["repeatType"]="0",["displayType"]="1",["backgroundText"]="The Guildmaster of the Fighters Guild wants me to perform a task for her.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3737",["name"]="In With the Tide",["level"]="11",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Walks-in-Ash told me of an assault force landing in the Ashlands west of Ebonheart. She's asked me to make for the land coral maze known as Vivec's Antlers.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3927",["name"]="In His Wake",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="Riften",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The city of Riften has been devastated by Sinmur's assault. The town guard is overwhelmed trying to help all the refugees. They're asking everyone to help with the recovery effort.",["poiIndex"]="21",["isShareable"]="1"},
{["internalId"]="4267",["name"]="All's Fair",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Rothalens have fallen on hard times. Sarisa has designs on Bar-Neeus's stall in the central market.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5038",["name"]="The Truth about Spiders",["level"]="12",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a soldier outside a spider cave in Stonefalls. She's suffering from a spider bite she received while trying to search the Hollow.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3671",["name"]="To Fort Virak",["level"]="12",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Covenant has taken Fort Virak, a vital strategic chokepoint.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3633",["name"]="Evening the Odds",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Fort Virak",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've infiltrated the ruins beneath Fort Virak. Walks-in-Ash asked me to help secure our foothold within the fort's main courtyard.",["poiIndex"]="19",["isShareable"]="1"},
{["internalId"]="3634",["name"]="The General's Demise",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Fort Virak",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Fort Virak is ours. All that remains is to kill General Serien, the leader of the Covenant forces in Stonefalls.",["poiIndex"]="19",["isShareable"]="1"},
{["internalId"]="3635",["name"]="City at the Spire",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Holgunn warned me that Tanval will likely try to release the second Brother of Strife from Tormented Spire. To ascend the spire I'll need to speak to the grandmaster of House Dres in Kragenmoor.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3695",["name"]="Aggressive Negotiations",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Kragenmoor",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Tanval Indoril has fled towards the Tormented Spire with the Coral Heart in hand. He wants to unleash Sadal, the Brother of Strife, from his imprisonment. Kragenmoor's Grandmaster Omin Dres is the only one who can allow me access to the mountain.",["poiIndex"]="10",["isShareable"]="1"},
{["internalId"]="3789",["name"]="This One's a Classic",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Laughs-at-All is struggling to understand humor. He heard a rumor about a sacred Dark Elf performance from the distant past.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3696",["name"]="Saving the Son",["level"]="13",["type"]="0",["zone"]="Stonefalls",["objective"]="Kragenmoor",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've discovered where Grandmaster Omin Dres' son is being held captive. The Vanguard is holding his son, Sen, in the tombs west of Kragenmoor.",["poiIndex"]="10",["isShareable"]="1"},
{["internalId"]="3788",["name"]="Vengeance for House Dres",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The grandmaster of House Dres wants revenge against the Vanguard troops outside Kragenmoor. He's commanded me to slay their leader.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6414",["name"]="The Frozen Isle",["level"]="50",["type"]="5",["zone"]="Wrothgar",["objective"]="Dungeon: Icereach",["repeatType"]="0",["displayType"]="2",["backgroundText"]="The Skald-King dispatched a party of sword-thanes to investigate unnatural storms swirling around Icereach. Alas, a coven of witches killed most of the warriors. I must venture into the aged keep, kill the witches within, and search for survivors.",["poiIndex"]="59",["isShareable"]="1"},
{["internalId"]="3698",["name"]="To the Tormented Spire",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The grandmaster of House Dres has given me a key that should see me past the wards at Tormented Spire. Now all that remains is to make my way there.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3826",["name"]="Climbing the Spire",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="Tormented Spire",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Tanval Indoril is inside the Tormented Spire working to unleash the Brother of Strife. Tanval's Vanguard holds the approach. If we don't stop Tanval, he'll unleash Sadal and destroy Stonefalls.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="3837",["name"]="Opening the Portal",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="Tormented Spire",["repeatType"]="0",["displayType"]="10",["backgroundText"]="With Tanval's help, we've lowered the final ward guarding the prison of Sadal, the Brother of Strife. The mages who originally imprisoned Sadal linger on the Tormented Spire as spirits, angered by Tanval's ritual.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="5504",["name"]="The Skin Trade",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Dragonstar Caravan Company's job broker, Guruzug, has tasked me with traveling to Argent Mine to slay werewolves. A wealthy client will pay handsomely for their skins.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5518",["name"]="Meat for the Masses",["level"]="50",["type"]="1",["zone"]="Wrothgar",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The laborers helping to rebuild Orsinium need supplies, specifically fresh meat. The hills and valleys of Wrothgar are typically filled with beasts to hunt, but their numbers are dwindling due to poaching. I must help feed the city's workers.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3868",["name"]="Sadal's Final Defeat",["level"]="14",["type"]="0",["zone"]="Stonefalls",["objective"]="Tormented Spire",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've accomplished great deeds with Tanval Indoril, Holgunn, and Walks-in-Ash. We've returned Tanval's Vanguard to the Ebonheart Pact, and convinced the spirits of Tormented Spire to open Sadal's prison.",["poiIndex"]="16",["isShareable"]="1"},
{["internalId"]="5035",["name"]="Calling Hakra",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Lieutenant Slonn led a team of scholars to Pinepeak Caverns to awaken the spirit of Hakra, one of the five hundred companions, but the team hasn't returned.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4508",["name"]="Washed Ashore",["level"]="5",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A drunk named Shazeem seems to have lost his memory. He offered to sell me a trained monkey, claiming it can find buried treasure.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4466",["name"]="The Broken Spearhead",["level"]="4",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4344",["name"]="Like Moths to a Candle",["level"]="7",["type"]="0",["zone"]="Stros M'Kai",["objective"]="Saintsport",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Captain Kaleen wants me to locate Crafty Lerisa and convince her to join in on the heist.",["poiIndex"]="1",["isShareable"]="0"},
{["internalId"]="4514",["name"]="The Spearhead's Crew",["level"]="7",["type"]="0",["zone"]="Stros M'Kai",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Captain Kaleen's cabin girl, Nicolene, informed me that Kaleen's crew is ready to start the heist as soon as I'm done recruiting.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3980",["name"]="Research Subject",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A researcher named Nila feels terrible about her colleague's capture. The Worm Cult was torturing him at the far end of the cavern.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3978",["name"]="Tomb Beneath the Mountain",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="Pinepeak Cavern",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Netapatuu and his colleagues came to Pinepeak Caverns to consult with the spirit of Hakra, one of Ysgramor's Companions. Worm Cultists attacked and drove them out. The cultists cannot be allowed to gain control of Hakra's spirit.",["poiIndex"]="4",["isShareable"]="1"},
{["internalId"]="3981",["name"]="To Taarengrav",["level"]="50",["type"]="0",["zone"]="The Rift",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Netapatuu fears the Worm Cult may try to enslave Valdur's spirit, much as they tried with Hakra's.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4631",["name"]="Past Due",["level"]="50",["type"]="0",["zone"]="Alik'r Desert",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a note of debt in Coldrock Diggings.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6276",["name"]="Lost in Murkmire",["level"]="50",["type"]="0",["zone"]="Murkmire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Overseer Shiralas asked if I could head into the swamp and look for her missing wife, Varona, and their pet Drooler.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6282",["name"]="A Life in Carvings",["level"]="50",["type"]="0",["zone"]="Murkmire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a grave-stake lying next to a note indicating that it was stolen. I should find out who to return the grave-stake to.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6281",["name"]="Swamp Jelly Sonata",["level"]="50",["type"]="0",["zone"]="Murkmire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="An eccentric Argonian by the name of Listens-By-Smell asked me to help her fulfill a dream of hers, somehow involving creatures called swamp jellies.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5051",["name"]="The Last of Them",["level"]="50",["type"]="0",["zone"]="Rivenspire",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I ran into Blademaster Qariar in Northpoint's back alleys. He's hunting down the remaining vampires that were working with Lady Lleraya Montclair.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4551",["name"]="A Poisoned Heart",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="While wading through the swamp, I stumbled into a cloud of roaming poison gas and passed out. Two Imperials rescued me and asked me to help them clean it up.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5788",["name"]="Dark Anchors in Stonefalls",["level"]="50",["type"]="3",["zone"]="Stonefalls",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Cardea Gallus of the Fighters Guild learned of Worm Cult activity in Stonefalls.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5822",["name"]="Madness in Glenumbra",["level"]="50",["type"]="3",["zone"]="Glenumbra",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Alvur Baren of the Mages Guild wants me to recover a dangerous relic from Glenumbra.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4556",["name"]="Strength of the Father",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="In the depths of Atanaz Ruins I came across the corpse of Gordag, cooked in his armor by a beast that dwells there.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4560",["name"]="Riches Beyond Measure",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Zahra, a Khajiit investigating Gandranen Ruins, has asked for my assistance.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4554",["name"]="Shadowfen Smorgasbord",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The drunken cook, Angwe, is in trouble. She bet her hunter friend that she'd collect ingredients from beasts all over Shadowfen and cook him a feast in Hissmir. Now sober, she is afraid to venture out into the fen, for fear of these same creatures.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3678",["name"]="Trials of the Burnished Scales",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Hissmir",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Due to archaic rules, Captain Henaamo of the Aldmeri Dominion is allowed to undergo sacred Argonian trials at Hissmir. Success will allow him to commune with the Hist. To determine his true plans, I must pass the trials myself.",["poiIndex"]="5",["isShareable"]="1"},
{["internalId"]="3680",["name"]="Of Dubious Value",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Someone stole several relics from a cart in Hissmir. I offered to help Wareem find the culprit.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3679",["name"]="Dreams From the Hist",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Hissmir",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I completed the Trials of the Burnished Scales at Hissmir, which allows me to commune with the Hist. Doing so will give me insight why Captain Henaamo, a Dominion soldier, would undertake the trials.",["poiIndex"]="5",["isShareable"]="1"},
{["internalId"]="4388",["name"]="A Life of Privilege",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met a Breton in the middle of the swamps of Shadowfen. She seems to be lost.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4557",["name"]="Will of the Broken",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="In the depths of Broken Tusk, I found a Daedric Urn surrounded by bodies. One of them stirred when I took the urn.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3732",["name"]="Overrun",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Aldmeri Dominion has set up a staging area south of Hissmir in a fishing village called Murkwater.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3849",["name"]="A Final Release",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Bezeer believes his mother's remains lie in White Rose Prison, away from the swamp waters. Her spirit is unable to return to the Hist.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3822",["name"]="Schism",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Mud Tree Village",["repeatType"]="0",["displayType"]="0",["backgroundText"]="An Argonian outside Mud Tree Village asked me to find his brother, Miharil. He was in the nearby mine when Goblins overran it.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="3824",["name"]="A Last Reminder",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="While in the Goblin camp, I found a Nord corpse clutching a runestone that bears the name \"Hodmar Iron-Kettle.\"",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4390",["name"]="Unbridled Wealth",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Teegya, a traveling Argonian merchant, lost her guar and a valuable ruby to crocodiles in the swamp. She's too hurt to round them up, so she asked me to search for the ruby and her favorite guar, Wades-In-Muck.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3712",["name"]="Captive Souls",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A traveling merchant named Daeril fell prey to Worm Cult necromancers camped below Stillrise Village. His guards were killed, and their souls are now trapped in animus geodes throughout the cultists' camp.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3685",["name"]="The Thin Ones",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Stillrise Village",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met Kothringi from Stillrise Village who told me Worm Cult necromancers plague their village.",["poiIndex"]="3",["isShareable"]="1"},
{["internalId"]="3709",["name"]="The Bargain's End",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="Stillrise Village",["repeatType"]="0",["displayType"]="0",["backgroundText"]="After returning the soul gem array, Chieftain Suhlak made me the arbiter over the fate of Stillrise Village. Destroying the soul gem array will allow the villagers to die, but preserving it will trap them in their lifeless state.",["poiIndex"]="3",["isShareable"]="1"},
{["internalId"]="4391",["name"]="Catch the Lightning",["level"]="50",["type"]="0",["zone"]="Shadowfen",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="A researcher and his apprentice are experimenting with an amulet that protects the wearer from wamasu lightning.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6269",["name"]="The Black Gauntlet",["level"]="50",["type"]="5",["zone"]="Murkmire",["objective"]="Arena: Blackrose Prison",["repeatType"]="1",["displayType"]="2",["backgroundText"]="Erilthel has requested that I help save her boss Seeks-the-Dark by completing a gauntlet held by the Blackguards. I'll have to make my way through the many foes of Blackrose Prison and emerge victorious.",["poiIndex"]="8",["isShareable"]="0"},
{["internalId"]="4726",["name"]="Siege Warfare",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've reached the siege range. Bugbesh told me to try each weapon. I should also talk to Marc and Jaleel to learn more about the weapons.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4727",["name"]="Reporting for Duty",["level"]="50",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Aurorelle Varin says there is much more for me to learn about the war in Cyrodiil.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4152",["name"]="Swamp to Snow",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="After events in Shadowfen, Vicecanon Heita-Meen and I must travel to Eastmarch, in Skyrim.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5479",["name"]="A Cold Wind From the Mountain",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I accomplished much since coming to Wrothgar. Now, it seems Curator Umutha has one final request from the House of Orsimer Glories.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5466",["name"]="Tinker Trouble",["level"]="50",["type"]="0",["zone"]="Wrothgar",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="It seems that Raynor Vanos used the Dwarven power core I acquired for him in Zthenganaz to rebuild and modify a Dwarven Sphere. The construct went crazy and knocked out Kireth, and when she woke up both it and her brother were missing.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4316",["name"]="On a Dare",["level"]="34",["type"]="5",["zone"]="Eastmarch",["objective"]="Hall of the Dead",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Three friends have drunkenly wandered into the catacombs beneath Windhelm after having a good time at the Konunleikar. They won't leave the catacombs until they're reunited, but it's not safe for them to search the tunnels alone.",["poiIndex"]="37",["isShareable"]="1"},
{["internalId"]="4322",["name"]="Lost Crown",["level"]="35",["type"]="5",["zone"]="Eastmarch",["objective"]="Hall of the Dead",["repeatType"]="0",["displayType"]="0",["backgroundText"]="In the Hall of the Dead beneath Windhelm I met Malana. She plans to gain favor in King Jorunn's court by eliminating Lodorr, an ancient necromancer entombed in the Merethic Era.",["poiIndex"]="37",["isShareable"]="1"},
{["internalId"]="2871",["name"]="Scout Faregyl Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Faregyl Mine for the Pact.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="3101",["name"]="Capture Warden Farm",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Warden Farm for the Pact.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5221",["name"]="Kill Enemy Templars",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Grand Warlord Dortene has authorized a reward for any Covenant soldier who slays 20 enemy Templars.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6160",["name"]="Struck from Memory",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Dangerous Worm Cult tomes are circulating throughout Summerset. The Office of Divine Prosecution wants some of these forbidden books recovered and disposed of.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6087",["name"]="Run Aground",["level"]="50",["type"]="1",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Office of Divine Prosecution tasked me with killing Keelsplitterâ€”a massive sea adder bred by the Sea Elves to hunt down High Elf ships. They also need me to recover intelligence logs from the ships Keelsplitter destroyed.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6202",["name"]="Sinking Summerset",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Battlereeve Tanerline charged me with finding allies to attack the abyssal geysers and destroy the pearls that power them before the Sea Sload cabal's plan to sink Summerset comes to fruition.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2638",["name"]="The Elder Scroll of Mnem",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="Grand Warlord Dortene asked for warriors to help capture the Elder Scroll of Mnem.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6085",["name"]="Birds of a Feather",["level"]="50",["type"]="1",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Divine Prosecution has requested I defeat the gryphons Haeliata and Nagravia. I'll also need to collect their feathers.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6086",["name"]="Never Forgotten",["level"]="50",["type"]="1",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Office of Divine Prosecution suffered a number of casualties to a creature called Graveld in Sil-Var-Woad.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6158",["name"]="Relic Runaround",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Divine Prosecution has tasked me to recover several stolen relics from a group known as the Sagacians. In addition, I'll need to collect any plans I find outlining the group's movements.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6082",["name"]="The Sickening Sea",["level"]="50",["type"]="1",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Divine Prosecution has tasked me with taking coral crab eggs from the reef between Lillandril and the Corgrad Wastes for the Sapiarchs to inspect for signs of corruption.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3097",["name"]="Capture Castle Roebeck",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Castle Roebeck for the Pact.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="3134",["name"]="Capture Faregyl Mine",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Faregyl Mine for the Pact.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6152",["name"]="Pilgrimage's End",["level"]="50",["type"]="0",["zone"]="Summerset",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Justiciar Tanorian wants me to recover articles of faith on behalf of the cult of Phynaster.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4723",["name"]="Siege Warfare",["level"]="38",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I've reached the siege range. Adalmor advised me to try each weapon. I should also talk to Looks-Forward and Lirtis to learn more about the weapons.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4724",["name"]="Reporting for Duty",["level"]="39",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Galsi Mavani says I have more to learn about the conflict in Cyrodiil.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4420",["name"]="Keepsake",["level"]="39",["type"]="0",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="In the confusion of battle, Sylvian Herius left a family heirloom in the Imperial-held cathedral across the river.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="2875",["name"]="Scout Bloodmayne Mine",["level"]="39",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to scout Bloodmayne Mine for the Pact.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="4255",["name"]="Ensuring Security",["level"]="39",["type"]="0",["zone"]="Auridon",["objective"]="Vulkhel Guard",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I arrived in the city of Vulkhel Guard, gateway to Auridon. Security is tight as Queen Ayrenn, the new leader of the Aldmeri Dominion, prepares to make a public appearance.",["poiIndex"]="17",["isShareable"]="1"},
{["internalId"]="4256",["name"]="A Hostile Situation",["level"]="39",["type"]="0",["zone"]="Auridon",["objective"]="Vulkhel Guard",["repeatType"]="0",["displayType"]="10",["backgroundText"]="At the request of the captain of the Vulkhel Watch, I've been running errands all over Auridon's port city. I even helped deal with a criminal named Fasion who claimed that Captain Astanya is working with a group called the Veiled Heritance.",["poiIndex"]="17",["isShareable"]="1"},
{["internalId"]="4211",["name"]="To Tanzelwil",["level"]="39",["type"]="0",["zone"]="Auridon",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Queen Ayrenn asked me to join her at the ancient site Tanzelwil where she must commune with the dead. She's worried there may be â€¦ complications.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4217",["name"]="In the Name of the Queen",["level"]="39",["type"]="0",["zone"]="Auridon",["objective"]="Tanzelwil",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Queen Ayrenn seeks to perform rituals honoring her ancestors. When she arrived at Tanzelwil, however, she found the spirits angered and violent.",["poiIndex"]="2",["isShareable"]="1"},
{["internalId"]="4222",["name"]="Rites of the Queen",["level"]="40",["type"]="0",["zone"]="Auridon",["objective"]="Tanzelwil",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Queen Ayrenn must perform the final ritual at Tanzelwil in the ancestral crypts. Malicious spirits plague this holy shrine and the traitorous Norion lurks somewhere in the shadows.",["poiIndex"]="2",["isShareable"]="1"},
{["internalId"]="4997",["name"]="One of the Undaunted",["level"]="45",["type"]="0",["zone"]="Stonefalls",["objective"]="",["repeatType"]="0",["displayType"]="2",["backgroundText"]="Kailstig the Axe has invited me to join a group of adventurers called the Undaunted.",["poiIndex"]="294967291",["isShareable"]="0"},
{["internalId"]="5928",["name"]="Relics of Ebernanit",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Numani-Rasi of the Urshilaku Ashlander tribe has requested that I explore the ruins of the Daedric shrine Ebernanit and gather as many historic relics as I can find.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5911",["name"]="Ash-Eater Hunt",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Huntmaster Sorim-Nakar of the Erabenimsun Ashlander tribe has given me a challenge. He wants me to hunt the dire ash beetle called Ash-Eater.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6040",["name"]="A Sticky Solution",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Razgurug requested I gather gather the husks of Clockwork trees for an adhesive. I'll also need to collect artificial coal to help keep the sap warm.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4786",["name"]="The Innkeeper's Daughter",["level"]="50",["type"]="0",["zone"]="Grahtwood",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Nellor, an innkeeper in Redfur Trading Post, informed me of bandits who kidnapped his daughter Larasel. He asked me to bring her home safely.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4954",["name"]="Light in the Darkness",["level"]="50",["type"]="0",["zone"]="Grahtwood",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met an old scholar near the Ayleid ruins who came to power a crystal for his research, but found the ruins swarming with Worm Cultists.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4857",["name"]="The Concealing Veil",["level"]="33",["type"]="0",["zone"]="Rivenspire",["objective"]="Lorkrata Hills",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Verandis completed his research and discovered that his missing memories lead to an ancient ruin in the Lorkrata Hills. He needs my help to discover the secret hidden within the ruins.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="4058",["name"]="Shadows Over Windhelm",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Windhelm",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I arrived in Eastmarch, in the Nord city of Windhelm. The place bustles with revelers as a celebration honoring the Skald-King is underway.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="4059",["name"]="The Konunleikar",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Windhelm",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Thane Mera has named me the Champion of Windhelm and wants me to enter the Footrace of the Nine Holds.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="4060",["name"]="Windhelm's Champion",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Windhelm",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Thane Mera, enraged by the death of the racer Hadring, demands that the assassins pay for this crime. She wants me, Windhelm's Champion, to deal with thisâ€”personally.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="4061",["name"]="One Victor, One King",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Windhelm",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Fildgor, King Jorunn's exiled brother, has returned to Skyrim. Nords from the Stormfist clan, loyal to Fildgor, have infiltrated the Konunleikar to get close to King Jorunn.",["poiIndex"]="7",["isShareable"]="1"},
{["internalId"]="4018",["name"]="Giant Problems",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Lower Yorgrim",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Giants have attacked the village of Lower Yorgrim, destroying everything in their path.",["poiIndex"]="5",["isShareable"]="1"},
{["internalId"]="4043",["name"]="Collector of Pelts",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="After giants and wolves overran the village of Lower Yorgrim, the survivors found shelter in a tiny fishing camp outside the village walls.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4037",["name"]="A Brother's Revenge",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Voljar's Meadery",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Yraldar Snowpeak was wounded trying to drive the Stormfist soldiers away from the meadery. He wants their leader, an Orc named Kazok, dead and his head on a pike. He asked for my help.",["poiIndex"]="32",["isShareable"]="1"},
{["internalId"]="4034",["name"]="A Friend in Mead",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Voljar's Meadery",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Stormfist soldiers ransacked Voljar's Meadery and slaughtered the familyâ€”all because they wanted free mead. Haraldar's wife was one of the Voljars killed in the attack. He wants to reopen the meadery, and he recruited his four brothers to help.",["poiIndex"]="32",["isShareable"]="1"},
{["internalId"]="4149",["name"]="Party Planning",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Rigurt the Brash has been named Master of Ceremonies for the celebration of King Jorunn's anniversary. Rigurt has many organizational duties to attend to and needs a little help.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4197",["name"]="Sounds of Alarm",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="A runner arrived from Fort Morvunskar, an installation to the west. Thane Mera believes Fildgor's forces have launched an attack against the fort.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4062",["name"]="Blindsided",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Fort Morvunskar",["repeatType"]="0",["displayType"]="10",["backgroundText"]="A renegade clan of Nords known as the Stormfists launched a surprise attack against Fort Morvunskar.",["poiIndex"]="8",["isShareable"]="1"},
{["internalId"]="4071",["name"]="Sleep for the Dead",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Fort Morvunskar",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The orders from Fildgor Orcthane suggest that the Stormfists are after something precious in the royal tomb. The spirit of Queen Nurnhilde and Lieutenant Koruni both asked me to return the relics. While I'm at it, I'll see what else is going on.",["poiIndex"]="8",["isShareable"]="1"},
{["internalId"]="4118",["name"]="Dark Deeds",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Bonestrewn Crest",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Draugr have spilled from the barrow beneath Bonestrewn Crest. Now they roam the area, attacking the living.",["poiIndex"]="12",["isShareable"]="1"},
{["internalId"]="4177",["name"]="Victory at Morvunskar",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Lieutenant Koruni asked me to deliver a dispatch detailing Fort Morvunskar's status to Thane Oda Wolf-Sister at Fort Amol.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4158",["name"]="The Pride of a Prince",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Fort Amol",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Stormfist spies have used Daedric alchemy to infiltrate Fort Amol. The King and the Pact delegates are in danger. I need to convince someone that the threat is real before it's too late.",["poiIndex"]="13",["isShareable"]="1"},
{["internalId"]="4166",["name"]="The War Council",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Fort Amol",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Thane Harvald want Lord Vurlop and I to go into the keep and check on King Jorunn. We need to make sure that the King and his war council are safe.",["poiIndex"]="13",["isShareable"]="1"},
{["internalId"]="4205",["name"]="Our Poor Town",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Trolls overran the town of Darkwater Crossing and now the Argonians who lived there have taken refuge in a nearby cave.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4075",["name"]="A Right to Live",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Wittestadr",["repeatType"]="0",["displayType"]="10",["backgroundText"]="I met a frightened young woman. Her and her friend were attacked by monsters on their way back to Windhelm.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="4106",["name"]="The Better of Two Evils",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Wittestadr",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Valeric believes vampires don't have to be monsters. He wants my help to stop his father, Majorn, who plans to create an army of bloodfiends.",["poiIndex"]="11",["isShareable"]="1"},
{["internalId"]="4203",["name"]="Lifeline",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Thane Oda Wolf-Sister believes that the Ternion monks might be able to heal King Jorunn. But their complex rituals require special components.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="4115",["name"]="Eternal Slumber",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Mistwatch",["repeatType"]="0",["displayType"]="10",["backgroundText"]="Jorunn the Skald-King has been poisoned. Now he lies in an eternal slumber. The Ternion monks, who revere the Three Old Gods, attempt to cure him. But time is running out.",["poiIndex"]="25",["isShareable"]="1"},
{["internalId"]="4123",["name"]="Gods Save the King",["level"]="50",["type"]="0",["zone"]="Eastmarch",["objective"]="Mistwatch",["repeatType"]="0",["displayType"]="10",["backgroundText"]="The Dreamwalker is lost in King Jorunn's nightmares. I must enter the Dreamstride and try to rescue both of them before the Daedric poison kills the King.",["poiIndex"]="25",["isShareable"]="1"},
{["internalId"]="3086",["name"]="Capture Fort Ash",["level"]="50",["type"]="7",["zone"]="Cyrodiil",["objective"]="",["repeatType"]="1",["displayType"]="0",["backgroundText"]="I accepted a mission to help capture Fort Ash for the Pact.",["poiIndex"]="294967291",["isShareable"]="1"},
