{["internalId"]="6499",["name"]="The Scraps",["level"]="50",["type"]="1",["zone"]="Western Skyrim",["objective"]="",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The High Elf Neramo needs to acquire special Dwarven power nodes and a master scepter in order to conduct his experiment. He asked for my help retrieving them.",["poiIndex"]="294967291",["isShareable"]="1"},
