{["internalId"]="2561",["name"]="A Family Affair",["level"]="50",["type"]="0",["zone"]="Stormhaven",["objective"]="Nurin Farm",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Nurin family needs help. They awoke this morning to find their farm burning, overrun by dremora, and the father, William Nurin, is missing. I volunteered to help find him.",["poiIndex"]="15",["isShareable"]="1"},
{["internalId"]="2578",["name"]="Scamp Invasion",["level"]="50",["type"]="0",["zone"]="Stormhaven",["objective"]="Nurin Farm",["repeatType"]="0",["displayType"]="0",["backgroundText"]="The Supernal Dreamers have apparently built a few portals on the Nurin's farm through which they are summoning scamps and possibly other Daedra. I've agreed to destroy these portals.",["poiIndex"]="15",["isShareable"]="1"},
