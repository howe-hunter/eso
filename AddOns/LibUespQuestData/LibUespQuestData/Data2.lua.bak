{["internalId"]="3964",["name"]="Secrets of the Lost City",["level"]="44",["type"]="5",["zone"]="Alik'r Desert",["objective"]="Lost City of the Na-Totambu",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I met a Redguard named Paldeen, held prisoner by the Khajiit bandits in the Lost City of the Na-Totambu. He asked for my help in opening the sealed inner chamber.",["poiIndex"]="28",["isShareable"]="1"},
{["internalId"]="3970",["name"]="An Ill-Fated Venture",["level"]="44",["type"]="5",["zone"]="Alik'r Desert",["objective"]="Lost City of the Na-Totambu",["repeatType"]="0",["displayType"]="0",["backgroundText"]="I found a letter on a dead man named Tavo in the Lost City of the Na-Totambu. His wife, Nahrina, wanted him to recover some Yokudan relics  and return to Sentinel to pay off a debt. I can recover the relics in his stead.",["poiIndex"]="28",["isShareable"]="1"},
