{["internalId"]="6520",["name"]="Precious Bark",["level"]="50",["type"]="0",["zone"]="Western Skyrim",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="The Coterie of Organized Scholars has tasked me with collecting fresh spriggan bark.",["poiIndex"]="294967291",["isShareable"]="1"},
