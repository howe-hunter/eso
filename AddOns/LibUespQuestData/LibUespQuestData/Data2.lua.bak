{["internalId"]="5925",["name"]="Relics of Assarnatamat",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Numani-Rasi of the Urshilaku Ashlander tribe has requested that I explore the ruins of the Daedric shrine Assarnatamat and gather as many historic relics as I can find.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5918",["name"]="A Creeping Hunger",["level"]="50",["type"]="1",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="I've been hired by researchers to defeat a Hunger that's protecting a ruin they wish to study. I'm also required to gather relics scattered around the ruin.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="5961",["name"]="Planting Misinformation",["level"]="50",["type"]="0",["zone"]="Vvardenfell",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="A faction of bandits in a cavern called Zainsipilu are attempting to grow moon-sugar grass. I've been asked to re-direct their methods through misinformation.",["poiIndex"]="294967291",["isShareable"]="1"},
