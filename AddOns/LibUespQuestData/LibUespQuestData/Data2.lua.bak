{["internalId"]="5636",["name"]="Pledge: Ruins of Mazzatun",["level"]="50",["type"]="15",["zone"]="Shadowfen",["objective"]="Dungeon: Ruins of Mazzatun",["repeatType"]="2",["displayType"]="2",["backgroundText"]="Urgarlag Chief-bane of the Undaunted has challenged me to explore the Ruins of Mazzatun.",["poiIndex"]="62",["isShareable"]="0"},
{["internalId"]="5246",["name"]="Pledge: Banished Cells II",["level"]="50",["type"]="15",["zone"]="Auridon",["objective"]="Dungeon: The Banished Cells II",["repeatType"]="2",["displayType"]="2",["backgroundText"]="Maj al-Ragath of the Undaunted has challenged me to delve the Banished Cells beneath Auridon.",["poiIndex"]="61",["isShareable"]="0"},
{["internalId"]="6363",["name"]="Tangled Tea Leaves",["level"]="50",["type"]="0",["zone"]="Northern Elsweyr",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Nisuzi's Baandari caravan suffers a tea shortage, but not just any tea will do as a replacement. Apparently, rare Samar tea shrubs grow in the Tangle, and the pekoe leaves they produce are the finest in Tamriel.",["poiIndex"]="294967291",["isShareable"]="1"},
{["internalId"]="6237",["name"]="Grave Mementos",["level"]="50",["type"]="0",["zone"]="Murkmire",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Bolu of the Dead-Water tribe, which calls itself the Naga-Kur, wants me to go to Bok-Xul and accomplish the task he set before me.",["poiIndex"]="294967291",["isShareable"]="1"},
