{["internalId"]="4146",["name"]="A Family Divided",["level"]="50",["type"]="5",["zone"]="Deshaan",["objective"]="Forgotten Crypts",["repeatType"]="0",["displayType"]="0",["backgroundText"]="Survivors of the plague from Narsis have built a refugee camp inside long-abandoned crypts. Sela and her husband came to join the group, but became separated.",["poiIndex"]="20",["isShareable"]="1"},
{["internalId"]="6041",["name"]="Enchanted Accumulation",["level"]="50",["type"]="0",["zone"]="Clockwork City",["objective"]="",["repeatType"]="2",["displayType"]="0",["backgroundText"]="Razgurug requested I gather a magical substance from the runestones in Clockwork City. I'll also need to collect a special crystal that causes the residue to glow.",["poiIndex"]="294967291",["isShareable"]="1"},
